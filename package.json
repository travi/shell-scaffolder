{
  "name": "@travi/shell-scaffolder",
  "description": "opinionated scaffolder for shell projects",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/shell-scaffolder",
  "bugs": "https://github.com/travi/shell-scaffolder/issues",
  "homepage": "https://npm.im/@travi/shell-scaffolder",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "sideEffects": false,
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:publish": "publint --strict",
    "test:unit:base": "DEBUG=any vitest run",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.2.0",
    "@travi/language-scaffolder-prompts": "2.0.1"
  },
  "devDependencies": {
    "@travi/any": "3.1.2",
    "@travi/eslint-config": "5.0.11",
    "@travi/eslint-config-mocha": "1.0.25",
    "ban-sensitive-files": "1.10.7",
    "c8": "10.1.3",
    "commitlint-config-travi": "1.4.55",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.1.7",
    "jest-when": "3.7.0",
    "npm-run-all2": "7.0.2",
    "publint": "0.3.3",
    "remark-cli": "12.0.1",
    "remark-preset-lint-travi": "6.0.7",
    "rimraf": "6.0.1",
    "rollup": "4.34.2",
    "rollup-plugin-auto-external": "2.0.0",
    "vitest": "3.0.5"
  }
}