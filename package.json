{
  "name": "@travi/shell-scaffolder",
  "description": "opinionated scaffolder for shell projects",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "module",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/shell-scaffolder",
  "bugs": "https://github.com/travi/shell-scaffolder/issues",
  "homepage": "https://npm.im/@travi/shell-scaffolder",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "sideEffects": false,
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "lint:publish": "publint",
    "test:unit:base": "DEBUG=any vitest run",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "pretest": "run-s build",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build",
    "prepare": "husky install"
  },
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/overridable-prompts": "^1.2.0",
    "@travi/language-scaffolder-prompts": "1.3.0"
  },
  "devDependencies": {
    "@travi/any": "2.1.8",
    "@travi/eslint-config": "3.0.29",
    "@travi/eslint-config-mocha": "1.0.19",
    "ban-sensitive-files": "1.9.19",
    "c8": "7.13.0",
    "commitlint-config-travi": "1.4.30",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "jest-when": "3.5.2",
    "npm-run-all": "4.1.5",
    "publint": "0.1.11",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "5.0.1",
    "rimraf": "5.0.0",
    "rollup": "3.22.0",
    "rollup-plugin-auto-external": "2.0.0",
    "vitest": "0.31.1"
  }
}
