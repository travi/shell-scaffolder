{
  "name": "@travi/shell-scaffolder",
  "description": "opinionated scaffolder for shell projects",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "repository": "travi/shell-scaffolder",
  "bugs": "https://github.com/travi/shell-scaffolder/issues",
  "homepage": "https://npm.im/@travi/shell-scaffolder",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "lint:sensitive": "ban",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "clean": "rimraf lib/",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@travi/any": "2.0.15",
    "@travi/babel-preset": "3.0.36",
    "@travi/eslint-config-travi": "1.8.8",
    "ban-sensitive-files": "1.9.14",
    "chai": "4.2.0",
    "codecov": "3.8.1",
    "commitlint-config-travi": "1.3.11",
    "cz-conventional-changelog": "3.3.0",
    "husky": "5.0.4",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-travi": "1.3.11",
    "rimraf": "3.0.2",
    "rollup": "2.34.2",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "9.2.1"
  },
  "dependencies": {
    "@travi/language-scaffolder-prompts": "1.2.0",
    "inquirer": "^7.0.0",
    "mz": "^2.7.0"
  }
}
